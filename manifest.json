{
	"name": "Basic Extension Test",
	"description": "Basic Chromium Extension Test",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "hello.html",
		"default_icon": "my_icon.png"
	},
	"commands":{
		"_execute_browser_action":{
			"suggested_key":{
				"default": "Ctrl+Shift+F",
				"mac": "MacCtrl+Shift+F"
			},
			"description": "Opens hello.html"
		}
	}
	
}